{
  "manifest_version": 3,
  "name": "kurabiye",
  "version": "1.0",
  "permissions": [
    "cookies",
    "activeTab",
    "storage",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon.png"
    }
  }
}